<div class="col-md-4">
  <div class="card card-profile">
    <div class="card-avatar">
      <%= link_to user_path(user) do %>
        <%= image_tag user.avatar_url(:thumb), class: 'img' %>
      <% end %>
    </div>

    <div class="content">
      <%= content_tag(:h6, t('.editor'), class: 'category text-gray') %>
    </div>
  </div>
</div>

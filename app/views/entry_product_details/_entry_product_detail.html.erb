<% detail = entry_product_detail %>
<tr>
  <%= content_tag(:td, detail.provider_product.name + ', ' + detail.provider_product.presentation) %>
  <%= content_tag(:td, detail.bulk) %>
  <%= content_tag(:td, detail.tare) %>
  <%= content_tag(:td, detail.batch) %>
  <%= content_tag(:td, l(detail.expiration_date)) %>
  <%= content_tag(:td, detail.invoice_folio) %>
  <%= content_tag(:td, l(detail.invoice_date)) %>
</tr>
